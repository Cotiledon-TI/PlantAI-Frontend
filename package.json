{
  "name": "plantai",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "npm run lint:css && npm run lint:ts",
    "lint:css": "stylelint 'src/**/*.css' --fix",
    "lint:ts": "eslint 'src/**/*.{ts,tsx}' --fix",
    "lint:fix": "npm run lint:css:fix && npm run lint:ts:fix",
    "lint:css:fix": "stylelint 'src/**/*.css' --fix",
    "lint:ts:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "lint:check": "npm run lint:css:check && npm run lint:ts:check",
    "lint:css:check": "stylelint 'src/**/*.css'",
    "lint:ts:check": "eslint 'src/**/*.{ts,tsx}'",
    "docker:build": "docker build -t chirmus/plantai-frontend:v3 --platform=linux/amd64 --build-arg VITE_API_URL=http://52.15.197.28:8080 --build-arg VITE_URL_ENDPOINT_BACKEND=http://52.15.36.197.28:8080 .",
    "docker:publish": "docker push chirmus/plantai-frontend:v3"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@reduxjs/toolkit": "^2.3.0",
    "@types/react-router-dom": "^5.3.3",
    "bootstrap": "^5.3.3",
    "file-type-checker": "^1.1.2",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.7",
    "react-bootstrap-icons": "^1.11.4",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^7.0.2",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "stylelint": "^16.12.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-order": "^6.0.4",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.2"
  }
